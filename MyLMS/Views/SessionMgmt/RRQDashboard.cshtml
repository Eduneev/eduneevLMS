
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>RRQDashboard</title>
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">

    <!-- Select2 -->
    <link rel="stylesheet" href="~/Template/plugins/select2/select2.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="~/Template/dist/css/AdminLTE.min.css">
    <!-- AdminLTE Skins. Choose a skin from the css/skins
         folder instead of downloading all of them to reduce the load. -->
    <link rel="stylesheet" href="~/Template/dist/css/skins/_all-skins.min.css">

    <script src="~/Template/plugins/jQuery/jquery-2.2.3.min.js"></script>

    <script src="~/Scripts/angular.min.js"></script>
    <script src="http://ajax.googleapis.com/ajax/libs/angularjs/1.0.3/angular-sanitize.js" type="text/javascript"></script>
    <script src="https://cdn.rawgit.com/AngularClass/angular-websocket/v2.0.0/dist/angular-websocket.min.js" type="text/javascript"></script>
    <script src="~/Scripts/AngularControllers/AngularModule.js"></script>
    <!-- Bootstrap 3.3.6 -->
    <link rel="stylesheet" href="~/Template/bootstrap/css/bootstrap.min.css">
    <script src="~/Scripts/AngularControllers/RRQDashboardController.js"></script>

    <!-- Bar Chat -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/1.0.2/Chart.js"></script>
    <script src="~/Template/BarChat/Chart.HorizontalBar.js"></script>
</head>
<body ng-app="MyModule">
    <div ng-controller="RRQDashboardCntrl">
        <!-- Form Body (Page body)-->
        <section class="content">
            <div class="row">
                <div class="col-xs-12">
                    <div class="nav-tabs-custom">
                        <div class="tab-content">
                            <!-- Tab 1 -->
                            <div class="tab-pane active" id="fa-icons">
                                <section id="new">
                                    <div>
                                        <div class="callout callout-warning">
                                            <div>
                                                <button type="button" id="{{Q.QID}}" ng-click="GetRRQQuestionAndOptions(Q.QID)" ng-repeat="Q in RRQQuestions" class="btn btn-outline btn-circle btn-xl">
                                                    <div style="font-size: 20px; font-weight:bold;">Q-{{Q.QUES_NO}}</div>
                                                </button>
                                                <button type="button" class="btn btn-outline btn-circle btn-xl right" style="float:right;">
                                                    <div style="font-size: 20px; font-weight:bold;" id="RRQName"></div>
                                                </button>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-4">
                                                <!-- AREA CHART -->
                                                <div class="box box-pane" style="height:450px;">
                                                    <div class="box-header">
                                                        <h3 class="box-title">Degree of Difficulty</h3>
                                                        <h5 class="box-title"> (% incorrect)</h5>
                                                        <div class="box-tools pull-right">
                                                            <button type="button" class="btn btn-box-tool" data-widget="collapse">
                                                                <i class="fa fa-minus"></i>
                                                            </button>
                                                            <button type="button" class="btn btn-box-tool" data-widget="remove"><i class="fa fa-times"></i></button>
                                                        </div>
                                                    </div>
                                                    <div class="box-body chart-responsive" style="height:420px; border-top: 3px solid #0c75db;">
                                                        <div>
                                                            <div>
                                                                <canvas id="canvas" height="420" width="600"></canvas>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <!-- DONUT CHART -->
                                                <div class="box">
                                                    <div class="box-header with-border">
                                                        <h3 class="box-title">Question Response</h3>
                                                        <h3 class="box-title"> Q-{{QuestionList[0].Question.QUES_NO}}</h3>
                                                        <div class="box-tools pull-right">
                                                            <button type="button" class="btn btn-box-tool" data-toggle="modal" data-target="#QRModal"><i class="fa fa-search-plus"></i></button>

                                                            <button type="button" class="btn btn-box-tool" data-widget="collapse">
                                                                <i class="fa fa-minus"></i>
                                                            </button>
                                                            <button type="button" class="btn btn-box-tool" data-widget="remove"><i class="fa fa-times"></i></button>
                                                        </div>
                                                    </div>
                                                    <div class="box-body chart-responsive" style="border-top: 3px solid #ff0000">
                                                        <div class="col-md-6">
                                                            <table class="table table-bordered" style="height:225px; font-size:20px;">
                                                                <tbody>
                                                                    <tr>
                                                                        <th>No Response: </th>
                                                                        <th> {{NoResponsesPrcnt}}%</th>
                                                                    </tr>
                                                                    <tr>
                                                                        <th>Responses: </th>
                                                                        <th> {{ResponsesPrcnt}}%</th>
                                                                    </tr>

                                                                    <tr>
                                                                        <th>Correct: </th>
                                                                        <th> {{CorrectPrcnt}}%</th>
                                                                    </tr>
                                                                    <tr>
                                                                        <th>Incorrect: </th>
                                                                        <th> {{InCorrestPrcnt}}%</th>
                                                                    </tr>
                                                                    <tr>
                                                                        <th>Correct Choice: </th>
                                                                        <th>
                                                                            <div ng-if="RevealAnswer == false">
                                                                                <button type="button" ng-click="MarkAnswer()" class="btn btn-block btn-success btn-flat" style="font-size:18px">?</button>
                                                                            </div>
                                                                            <div ng-if="CorrectOption == 'A' && RevealAnswer==true">
                                                                                <button type="button" ng-click="GetStudentsByResponse(1)" class="btn btn-block btn-success btn-flat" style="font-size:18px">{{CorrectOption}}</button>
                                                                            </div>
                                                                            <div ng-if="CorrectOption == 'B' && RevealAnswer==true">
                                                                                <button type="button" ng-click="GetStudentsByResponse(2)" class="btn btn-block btn-success btn-flat" style="font-size:18px">{{CorrectOption}}</button>
                                                                            </div>
                                                                            <div ng-if="CorrectOption == 'C' && RevealAnswer==true">
                                                                                <button type="button" ng-click="GetStudentsByResponse(3)" class="btn btn-block btn-success btn-flat" style="font-size:18px">{{CorrectOption}}</button>
                                                                            </div>
                                                                            <div ng-if="CorrectOption == 'D' && RevealAnswer==true">
                                                                                <button type="button" ng-click="GetStudentsByResponse(4)" class="btn btn-block btn-success btn-flat" style="font-size:18px">{{CorrectOption}}</button>
                                                                            </div>
                                                                        </th>
                                                                    </tr>
                                                                </tbody>
                                                            </table>
                                                        </div>
                                                        <div class="col-md-6">
                                                            <div class="box box-default">
                                                                <div class="box-body text-center ">
                                                                    <div id="bar-chart" style="height: 200px;">
                                                                        <div class="progress vertical" style="width:26px;">
                                                                            <strong>A</strong>
                                                                            <div class="progress-bar progress-bar-green" role="progressbar" aria-valuenow="40" aria-valuemin="0" aria-valuemax="100" style="height: {{OPTION_1_PRCNT}}%">
                                                                                <span class="sr-only">{{OPTION_1_PRCNT}}</span>
                                                                                <strong>{{OPTION_1_PRCNT}}%</strong>
                                                                            </div>
                                                                        </div>
                                                                        <div class="progress vertical" style="width:26px;">
                                                                            <strong>B</strong>
