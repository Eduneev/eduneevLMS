                                                    <div class="box-body chart-responsive" style="border-top: 3px solid #0c75db;">
                                                        <div style="background-color:#808080">
                                                            <button type="button" id="A" ng-click="GetStudentsByResponse(1)" class="btn btn-outline btn-circle btn-xl">
                                                                A
                                                            </button>
                                                            <button type="button" id="A" ng-click="GetStudentsByResponse(2)" class="btn btn-outline btn-circle btn-xl">
                                                                B
                                                            </button>
                                                            <button type="button" id="A" ng-click="GetStudentsByResponse(3)" class="btn btn-outline btn-circle btn-xl">
                                                                C
                                                            </button>
                                                            <button type="button" id="A" ng-click="GetStudentsByResponse(4)" class="btn btn-outline btn-circle btn-xl">
                                                                D
                                                            </button>
                                                        </div>

                                                        <table class="table table-hover table-bordered">
                                                            <thead>
                                                                <tr>
                                                                    <th>FFF</th>
                                                                    <th>Name</th>
                                                                    <th>Center</th>
