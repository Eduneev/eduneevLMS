                                                                <i class="fa fa-minus"></i>
                                                            </button>
                                                            <button type="button" class="btn btn-box-tool" data-widget="remove"><i class="fa fa-times"></i></button>
                                                        </div>
                                                    </div>
                                                    <div class="box-body chart-responsive" style="min-height:405px;border-top: 3px solid #ff6a00;">
                                                        <table class="table table-hover table-bordered" style="height:370px">
                                                            <tbody ng-repeat="Q in QuestionList">
                                                                <tr>
                                                                    <td style="font-size:20px;">
                                                                        <span><strong>Q-{{Q.Question.QUES_NO}}. {{Q.Question.QuestionText}}</strong></span>
                                                                    </td>
                                                                </tr>
                                                                <tr>
                                                                    <td colspan="3">
                                                                        <div style="padding:5px; font-weight:bold;">
                                                                            <table ng-repeat="O in Q.Options" class="table">
                                                                                <tr style="border:1px solid #808080;">
                                                                                    <td style="background-color:darkseagreen; width:20px; padding-left:4px; font-size:25px;">{{O.OptionChar}} </td>
                                                                                    <td style="padding-left:5px; font-size:20px;">{{O.OptionText}}</td>
                                                                                </tr>
                                                                            </table>
                                                                        </div>
                                                                    </td>
                                                                </tr>
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                </div>
                                                <!-- BAR CHART -->
                                                @*<div class="box box-success collapsed-box">*@
                                                <div class="row">
                                                    <div class="col-md-12">
                                                        <!-- Custom Tabs -->
                                                        <div class="nav-tabs-custom">
                                                            <ul class="nav nav-tabs">
                                                                <li class="active"><a href="#tab_1" data-toggle="tab">Top 10</a></li>
                                                                <!--<li><a href="#tab_2" data-toggle="tab">FFF</a></li>-->
                                                                <div class="box-tools pull-right">
                                                                    <button type="button" class="btn btn-box-tool" data-toggle="modal" data-target="#T20Modal"><i class="fa fa-search-plus"></i></button>
                                                                    <button type="button" class="btn btn-box-tool" data-widget="collapse">
                                                                        <i class="fa fa-minus"></i>
                                                                    </button>
                                                                    <button type="button" class="btn btn-box-tool" data-widget="remove"><i class="fa fa-times"></i></button>
                                                                    </div>
                                                            </ul>
                                                            <div class="tab-content">
                                                                <div class="tab-pane active" id="tab_1">
                                                                    <div class="box-body chart-responsive" style="height:250px; overflow:auto;">
                                                                        <table class="table table-hover table-bordered">
                                                                            <thead>
                                                                                <tr>
                                                                                    <th>Student Name</th>
                                                                                    <th>Center Name</th>
                                                                                    <th>Score (%)</th>
                                                                                </tr>
                                                                            </thead>
                                                                            <tbody ng-repeat="Stud in Top10Students">
                                                                                <tr>
                                                                                    <td>
                                                                                        <span><strong>{{Stud.StudentName}}</strong></span>
                                                                                    </td>
                                                                                    <td>
                                                                                        <span><strong>{{Stud.CenterName}}</strong></span>
                                                                                    </td>
                                                                                    <td>
                                                                                        <span><strong>{{Stud.TotalMarks/Stud.TotalRRQMarks * 100}}</strong></span>
                                                                                    </td>
                                                                                </tr>
                                                                            </tbody>
                                                                        </table>
                                                                    </div>
                                                                </div>
                                                                <!-- /.tab-pane -->
                                                                <div class="tab-pane" id="tab_2">
                                                                    <div class="box-body chart-responsive" style="height:250px; overflow:auto;">
                                                                        <table class="table table-hover table-bordered">
                                                                            <thead>
                                                                                <tr>
                                                                                    <th>Sl No.</th>
                                                                                    <th>Student Name</th>
                                                                                    <th>Center Name</th>
                                                                                </tr>
                                                                            </thead>
                                                                            <tbody ng-repeat="Stud in Top10FastestStudents">
                                                                                <tr>
                                                                                    <td>
                                                                                        <span><strong>{{Stud.StudentName}}</strong></span>
                                                                                    </td>
                                                                                    <td>
                                                                                        <span><strong>{{Stud.CenterName}}</strong></span>
                                                                                    </td>
                                                                                </tr>
                                                                            </tbody>
                                                                        </table>
                                                                    </div>
                                                                </div>
                                                                <!-- /.tab-pane -->
                                                            </div>
                                                            <!-- /.tab-content -->
                                                        </div>
                                                        <!-- nav-tabs-custom -->
                                                    </div>
                                                </div>
