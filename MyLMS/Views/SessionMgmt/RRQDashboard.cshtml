                            <button type="button" class="close" data-dismiss="modal">&times;</button>
                            <h1 class="text-info">Question Body</h1>
                        </div>
                        <div class="modal-body" style="margin:0px 10px 0px 10px;">
                            <div class="box-body">
                                <table class="table table-hover table-bordered" style="height:380px">
                                    <tbody ng-repeat="Q in QuestionList">
                                        <tr>
                                            <td style="font-size:30px;">
                                                <span><strong>Q-{{Q.Question.QUES_NO}}. {{Q.Question.QuestionText}}</strong></span>
                                            </td>
                                        </tr>
                                        <tr>
                                            <td colspan="3">
                                                <div style="margin-left:10px; padding:5px; font-size:30px; font-weight:bold;">
                                                    <table class="col-lg-12" ng-repeat="O in Q.Options">
                                                        <tr style="border:1px solid #808080;">
                                                            <td style="background-color:burlywood; width:20px; padding-left:4px;">{{O.OptionChar}}.</td>
                                                            <td style="padding-left:5px;">{{O.OptionText}} </td>
                                                        </tr>
                                                        <tr style="height:10px;">
                                                            <td></td>
                                                            <td></td>
                                                        </tr>
                                                    </table>
                                                </div>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                        </div>
                    </div>
                </div>
            </div>
            @* Popup to Show Course Window *@

            @* Popup to Question Response *@
            <div class="modal" id="T20Modal" role="dialog">
                <div class="modal-dialog" style="width:900px; height:800px;">
                    <div class="modal-content" style="width: 100%;">
                        <div class="modal-header">
                            <button type="button" class="close" data-dismiss="modal">&times;</button>
                            <h1 class="text-info">Top 10 Students</h1>
                        </div>
                        <div class="modal-body" style="margin:0px 10px 0px 10px;">
                            <div class="box-body">
                                <table class="table table-hover table-bordered" style="height:100px">
                                    <thead>
                                        <tr>
                                            <th>Student Name</th>
                                            <th>Center Name</th>
                                            <th>Score</th>
                                        </tr>
                                    </thead>
                                    
                                    <tbody ng-repeat="Stud in Top10Students">
                                        <tr style="font-size:25px; font-weight:bold;">
                                            <td>
                                                <span><strong>{{Stud.StudentName}}</strong></span>
                                            </td>
                                            <td>
                                                <span><strong>{{Stud.CenterName}}</strong></span>
                                            </td>
                                            <td>
                                                <span><strong>{{Stud.TotalMarks}}</strong></span>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                        </div>
                    </div>
                </div>
            </div>
            @* Popup to Show Course Window *@

        </section>

    </div>






