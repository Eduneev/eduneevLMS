                                            <tr>
                                                <th>Correct: </th>
                                                <th> {{CorrectPrcnt}}%</th>
                                            </tr>
                                            <tr>
                                                <th>Incorrect: </th>
                                                <th> {{InCorrestPrcnt}}%</th>
