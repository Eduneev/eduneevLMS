
@{
    ViewBag.Title = "NewRRQ";
    Layout = "~/Views/Shared/_PageLayout.cshtml";
}

@section scripts{
    <script src="~/Scripts/AngularControllers/RRQController.js"></script>
}
<div ng-controller="NewRRQCntrl">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            New RRQ
            <small>Create new RRQ</small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="/Home"><i class="fa fa-dashboard"></i>Home</a></li>
            <li><a href="#">New RRQ</a></li>
            @*<li class="active">Add Course</li>*@
        </ol>
    </section>

    <!-- Form Body (Page body)-->
    <section class="content">
        <div class="box box-default">
            <div class="box-body">
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group col-md-4">
                            <label>RRQ Name</label>
                            <input type="text" ng-model="RRQName" name="CenterName" style="font-weight:bold; color:coral;" class="form-control " placeholder="Enter RRQ name...">
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group col-md-4">
                            <label>RRQ Name</label>
                            <input type="text" ng-model="RRQName" name="CenterName" style="font-weight:bold; color:coral;" class="form-control " placeholder="Enter RRQ name...">
                        </div>
                    </div>
                </div>
            </div>
            <div class="box-footer">
                <button type="submit" ng-click="SaveCenter()" class="btn btn-primary">Create RRQ</button>
                <button type="submit" class="btn btn-default">Cancel</button>
            </div>
        </div>
    </section>
</div>
